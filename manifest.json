{
  "name": "bd_wishlist",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Wishlist button on results page of Bookdepository.",
  "content_scripts": [
    {
      "matches": ["http://*.bookdepository.com/*", "http://*.bookdepository.co.uk/*"],
      "js": ["jquery.min.js", "main.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabs"]
}
